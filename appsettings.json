{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*",
  "Jwt": {
    "Key": "ReplaceWithStrongSecretKey_DoNotCommit",
    "Issuer": "UserAuthApi",
    "Audience": "UserAuthApiClients",
    "ExpiryMinutes": 60
  },
  "ConnectionStrings": {
    "DefaultConnection": "Server=(localdb)\\mssqllocaldb;Database=UserAuthDb;Trusted_Connection=True;"
  }
}
